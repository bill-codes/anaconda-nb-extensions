#!/usr/bin/env python
from jupyter_core import paths
from anaconda_notebook import main

if __name__ == '__main__':
    # Get the config and data dir from the environment
    jupyter_config_dir = paths.ENV_CONFIG_PATH[0]
    jupyter_data_dir   = paths.ENV_JUPYTER_PATH[0]

    main(jupyter_config_dir, jupyter_data_dir)
